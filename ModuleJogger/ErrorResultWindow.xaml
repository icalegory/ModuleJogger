<Window x:Class="ModuleJogger.ErrorResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ModuleJogger"
        mc:Ignorable="d"
        Title="Error Result List" Height="450" Width="600"
        ContentRendered="Windows_ContentRendered"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="339" TextWrapping="Wrap" VerticalAlignment="Top" Width="592" ScrollViewer.HorizontalScrollBarVisibility="Disabled"
         ScrollViewer.VerticalScrollBarVisibility="Auto"/>
        <Button x:Name="button" Content="OK" HorizontalAlignment="Left" Margin="474,370,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>

    </Grid>
</Window>
